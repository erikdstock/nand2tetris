/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
    And(a=a, b=b, out=aAndB);
    Or(a=a, b=b, out=aOrB);
    Not(in=aAndB, out=notSame);
    And(a=aOrB, b=notSame, out=out);
}